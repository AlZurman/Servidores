<mvc:View controllerName="academia2022.zservidores.controller.Servidores"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt">

<Page id="page"  title="{i18n>titleRegiones}" showHeader="false">


    <HBox>
	    <items>
	        <core:Icon src="sap-icon://globe" alt="Cart" size="5rem" class=" sapUiTinyMarginTop sapUiTinyMarginBegin"/>
		    <Title text="Regiones" titleStyle="H1" class=" sapUiTinyMarginTop sapUiTinyMarginBegin"/>
    	</items>
	</HBox>
    
        <content>

<HBox class= "sapUiTinyMarginTop sapUiTinyMarginBegin">

	<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Servidores de la región argentina"
		frameType="OneByOne" press="press">
		<TileContent  footer="Argentina">
			<ImageContent  src="../controller/Flags/Argentina.png" />
		</TileContent>
	</GenericTile>

	<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Servidores de la región brasilera"
		frameType="OneByOne" press="press">
		<TileContent  footer="Brasil">
			<ImageContent  src="../controller/Flags/Brasil.png" />
		</TileContent>
	</GenericTile>

</HBox>

        </content>
    </Page>
</mvc:View>